<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<AnchorPane prefHeight="160.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
			fx:controller="michal.zawadzki.workdayappui.control.LoginController">
	<children>
		<GridPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="160.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0"
				  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
			</columnConstraints>
			<rowConstraints>
				<RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
				<RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
				<RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
				<RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
			</rowConstraints>
			<children>
				<Label text="Czy chcesz zatwierdzić wniosek urlopowy ? " GridPane.columnSpan="2" GridPane.halignment="CENTER"
					   GridPane.rowIndex="1">
					<GridPane.margin>
						<Insets left="20.0"/>
					</GridPane.margin>
					<padding>
						<Insets right="20.0"/>
					</padding>
				</Label>
				<Button fx:id="logInButton1" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
						onAction="#onLogInButtonClick" text="Zatwierdź" GridPane.rowIndex="2">
					<GridPane.margin>
						<Insets left="70.0"/>
					</GridPane.margin>
				</Button>
				<Button fx:id="logInButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
						onAction="#onLogInButtonClick" text="Odrzuć" GridPane.halignment="RIGHT" GridPane.rowIndex="2">
					<GridPane.margin>
						<Insets right="70.0"/>
					</GridPane.margin>
				</Button>
			</children>
		</GridPane>
	</children>
</AnchorPane>
